<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="ExtryzeAccentBrush" x:Name="ExtryzeAccentBrush" Color="#0079cb"/>
    <SolidColorBrush x:Key="ExtryzeAccentBrushSecondary" x:Name="ExtryzeAccentBrushSecondary" Color="#1D98EB"/>

    <Color x:Key="ExtryzeAccentBrushColour" A="#FF" R="#00" G="#79" B="#cb" />
    <Color x:Key="ExtryzeAccentBrushColourSecondary" A="#FF" R="#1D" G="#98" B="#EB" />
    
    <BitmapImage x:Key="ExtryzeAccentLogo" x:Name="ExtryzeAccentLogo" UriSource="/XBDMCom;component/Metro/Images/blue_logo_small.png" />
    <BitmapImage x:Key="ExtryzeAccentLogoLarge" x:Name="ExtryzeAccentLogoLarge" UriSource="/XBDMCom;component/Metro/Images/blue_logo.png" />
</ResourceDictionary>